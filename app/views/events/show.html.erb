<h1><%= @event.display_date %></h1><br>
<h1><%= @event.display_time %></h1><br>
<h3><%= @event.where %></h3><br>

<% if !@event.users.include? current_user %>
  <h1>not signed up</h1>
<% end %>

<%= link_to 'Sign up'  %>

<ul><h3>Attendees:</h3>
  <% @event.users.each do |user| %>
    <li><%= link_to user.name, user %></li>
  <% end %>
</ul>
